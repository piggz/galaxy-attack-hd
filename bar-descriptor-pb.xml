<?xml version='1.0' encoding='UTF-8' standalone='no'?>
<qnx xmlns="http://www.qnx.com/schemas/application/1.0">
    <id>uk.co.piggz.space_invaders_qt5</id>
    <filename>pgz-spaceinvaders-qt5</filename>
    <name>Space Invaders</name>
    <versionNumber>0.9.0</versionNumber>
    <buildId>4</buildId>
    <description>PGZ Space Invaders</description>
    <copyright>2012</copyright>
    <author>PGZ Solutions</author>
    <authorId>gYAAgOFP83zwpwdTrWwjCuLaPac</authorId>
    <initialWindow>
        <aspectRatio>landscape</aspectRatio>
        <autoOrients>false</autoOrients>
        <systemChrome>none</systemChrome>
        <transparent>false</transparent>
    </initialWindow>
    <env value="app/native/imports" var="QML2_IMPORT_PATH"/>
    <env value="app/native/plugins" var="QT_PLUGIN_PATH"/>
    <env value="app/native/lib" var="LD_LIBRARY_PATH"/>
    <arg>-style</arg>
    <arg>qnxlight</arg>
    <arg>-platform</arg>
    <arg>qnx</arg>
    <action system="true">run_native</action>
    <action>access_internet</action>
    <action>read_device_identifying_information</action>
    <!-- pgz-spaceinvaders-qt5 should point to the project binary, path can be relative -->
    <asset path="pgz-spaceinvaders-qt5" type="Qnx/Elf" entry="true">pgz-spaceinvaders-qt5</asset>
    <asset path="${SRC_DIR}/SLScoreFormatter.strings">scoreloop/SLScoreFormatter.strings</asset>
<!-- Required Qt Libraries -->
    <asset path="${QT_INSTALL_LIBS}/libQt5Concurrent.so.5">lib/libQt5Concurrent.so.5</asset>
    <asset path="${QT_INSTALL_LIBS}/libQt5Sensors.so.5">lib/libQt5Sensors.so.5</asset>
    <asset path="${QT_INSTALL_LIBS}/libQt5Sql.so.5">lib/libQt5Sql.so.5</asset>
    <asset path="${QT_INSTALL_LIBS}/libQt5Xml.so.5">lib/libQt5Xml.so.5</asset>
    <asset path="${QT_INSTALL_LIBS}/libQt5PrintSupport.so.5">lib/libQt5PrintSupport.so.5</asset>
    <asset path="${QT_INSTALL_LIBS}/libQt5OpenGL.so.5">lib/libQt5OpenGL.so.5</asset>
    <asset path="${QT_INSTALL_LIBS}/libQt5QuickParticles.so.5">lib/libQt5QuickParticles.so.5</asset>
    <asset path="${QT_INSTALL_LIBS}/libQt5Network.so.5">lib/libQt5Network.so.5</asset>
    <asset path="${QT_INSTALL_LIBS}/libQt5Qml.so.5">lib/libQt5Qml.so.5</asset>
    <asset path="${QT_INSTALL_LIBS}/libQt5Quick.so.5">lib/libQt5Quick.so.5</asset>
    <asset path="${QT_INSTALL_LIBS}/libQt5Gui.so.5">lib/libQt5Gui.so.5</asset>
    <asset path="${QT_INSTALL_LIBS}/libQt5Core.so.5">lib/libQt5Core.so.5</asset>
    <asset path="${QT_INSTALL_LIBS}/libQt5V8.so.5">lib/libQt5V8.so.5</asset>
    <asset path="${QT_INSTALL_LIBS}/libQt5XmlPatterns.so.5">lib/libQt5XmlPatterns.so.5</asset>
    <asset path="${QT_INSTALL_LIBS}/libQt5Widgets.so.5">lib/libQt5Widgets.so.5</asset>
    <asset path="${QT_INSTALL_LIBS}/libQt5Svg.so.5">lib/libQt5Svg.so.5</asset>
       
    <!--<asset path="${QT_INSTALL_LIBS}">lib</asset>-->
    
    <asset path="${QT_INSTALL_PLUGINS}">plugins</asset>
    <asset path="${QT_INSTALL_QML}">imports</asset>
    <asset path="${SRC_DIR}/qml">qml</asset>
    <asset path="/home/piggz/projects/pgz-spaceinvaders-qt5/data/128x128/pgz-spaceinvaders.png">pgz-spaceinvaders.png</asset>
    <!-- These values will be replaced when deploying in Qt Creator -->
    <icon>
        <image>pgz-spaceinvaders.png</image>
    </icon>
</qnx>
